<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andrew.test" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.getbase.floatingactionbutton.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".share"
            android:label="@string/title_activity_share" >
        </activity>
        <activity
            android:name=".DataActivity"
            android:label="@string/title_activity_data"
            android:parentActivityName="com.getbase.floatingactionbutton.sample.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.getbase.floatingactionbutton.sample.MainActivity" />
        </activity>
        <activity
            android:name=".ChartActivity"
            android:label="@string/title_activity_chart"
            android:parentActivityName="com.getbase.floatingactionbutton.sample.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.getbase.floatingactionbutton.sample.MainActivity" />
        </activity>
        <activity
            android:name=".SummaryActivity"
            android:label="@string/title_activity_summary"
            android:parentActivityName="com.getbase.floatingactionbutton.sample.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.getbase.floatingactionbutton.sample.MainActivity" />
        </activity>
    </application>

</manifest>
